<div class="card">
    <%= image_tag prototype.image, class: :card__img if prototype.image.attached? %>
  <%# <%= link_to image_tag("プロトタイプの画像", class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.prototype, root_path, class: :card__title%> 
    <p class="card__summary">
      <%= link_to prototype.catch_copy, root_path %>
    </p>
     <%=  link_to prototype.name, root_path, class: :card__user %> 
  </div>
</div>